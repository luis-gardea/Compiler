class Main {
    main() : Int {
    	{
        case true of 
        	x : SELF_TYPE => 1 + "";
        	c : SELF_TYPE => 3; 
    	esac;

        case true of 
        	x : SELF_TYPE => 1 + x;
        	c : Int => 3; 
    	esac;

    	self@Int.copy();
    	}
    };

    x(): SELF_TYPE {
    	self
    };
}; 

class A inherits Main {
    x() : A {
        new A
    };

    a(): A {
    	(new Main).x()
    };
	
};

class B {
    b(a:SELF_TYPE, b: SELF_TYPE): Int {
    	let x : SELF_TYPE <- self in x
    };
	
};
