class Main {
    a : Main <- a + 5;

    a : Main;

    a : Main;
    
    b(x : Main) : Main {
        b <- x
    };

    d : Main;

    c : Main <- if d then e <- 5 else e <- 6 fi;

    g : Main <- if d then e <- e + e else e fi;

    h: Main <- "" = 7;

    j() : Main {
        k <- 5
    };

    -- j: A <- new B;

    k: B;
};
