Class Main inherits IO {
    main() : Int { 3 };
};

class Y inherits Main {
    main() : String { "a" };
};

class V {
	do_nothing() : Int { 2 };
  	x : Object <- {
      case true of
            x : String => 5;
            self : Int => self.do_nothing();
            self : String => 5;
      esac;
      let self : Int, y:Int <- "Hi" in self.do_nothing();
    };
};