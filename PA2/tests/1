class Testgood {
    --nested if/else statements--
    test1(): Int {
        if a < b then
	        if b < c then
                1
	        else
		        if c < d then
                    2
		        else
                    3
		        fi
	        fi
        else
            4
        fi
    };

    --Simplest let statement (LET1)--
    test2(): Int {
        let a : Int in 
            3
    };

    --Simplest let statement (LET1)--
    test2(): Int {
        let a : Int in 
            3
    };

    --Simple let test (LET2)--
    test3(): Int {
        let a : Int <- 3 in
            b <- a + a
    };

    -- Nested let test1--
    test4(): Int {
        let a : Int <- 3 in
            let b : Int in
                c <- a + a
    };

    -- Nested let test2--
    test5(): Int {
        let a : Int <- let b : Int in 
            c <- a + a in
            let d : Int in
                c <- a + a
    };
};
